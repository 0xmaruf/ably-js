{
  "compilerOptions": {
    "target": "es3",
    "module": "commonjs",
    "lib": ["ES5", "DOM", "webworker"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
        "platform": ["./nodejs/platform", "./browser/fragments/platform-browser", "./browser/fragments/platform-reactnative"]
    }
  }
}
